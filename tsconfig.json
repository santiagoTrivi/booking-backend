{
  "compilerOptions": {
    "target": "es2016",                                 
    "module": "commonjs",                                
    "esModuleInterop": true,                            
    "forceConsistentCasingInFileNames": true,            
    "strict": true,
    "strictPropertyInitialization": false,                                     
    "skipLibCheck": true,
    "outDir": "./dist",
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true                        
  }
}
